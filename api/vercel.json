{
  "builds": [{ "src": "*.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "rewrites": [
    {
        "src": "/api/(.*)",
        "destination": "https://ticketify-api.vercel.app/$1"
    },
    {
        "src": "/(.*)",
        "destination": "/"
    }
]
}
